{
    "name": "Pics to Puppies",
    "version": "1.0",
    "description": "Replace pictures on a website with puppies",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent"],
    "icons": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "64": "icons/icon_64.png",
      "128": "icons/icon_128.png"
    },
    "page_action": {
      "default_icon": {
        "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "64": "icons/icon_64.png",
      "128": "icons/icon_128.png"
      }
    },
    "background": {
      "scripts": ["script.js"]
    }
}
